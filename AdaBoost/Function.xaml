<Page x:Class="AdaBoost.FUNCTION"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AdaBoost"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label Content="機能を選択" FontSize="20" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
            <ComboBox Name="func_combox" Width="300" Height="50" FontSize="20" ItemsSource="{Binding func}"
           SelectionChanged="FunctionProc" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
            <Button Content="決定" Width="150" Height="50" FontSize="20" Click="SelectClick"/>
        </StackPanel>
    </Grid>
</Page>
